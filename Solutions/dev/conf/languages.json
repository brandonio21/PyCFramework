{ 
	"variables" : {
		"filename"                : "{fileName}",
		"filename_less_extension" : "{fileNameWoExtension}",
		"directory"               : "{directory}"

	},

	"languages" : [
		{
			"language"         : "java",
			"compileExtension" : "java",
			"compileCommand"   : "javac {directory}/{fileName}",
			"runExtension"     : "class",
			"runCommand"       : "java -cp {directory} {fileNameWoExtension}"
		},

		{
			"language"         : "C++",
			"compileExtension" : "cpp",
			"compileCommand"   : "g++ -std=c++11 -o {directory}/{fileNameWoExtension}.o {directory}/{fileName}",
			"runExtension"     : "o",
			"runCommand"       : "{directory}/{fileName}"
		},

		{
			"language"         : "python",
			"runExtension"     : "py",
			"runCommand"       : "python {fileName}"
		}
	]
}
