{
  "file": {
    "header" : "{file/imports_java} public class Problem{problem} {{",
    "footer" : ["}}", "{file/jsonfast_java}"]
  },

  "user_impl": {
    "header" : "public static {returnType} {methodName}({formalsList}) {{",
    "body"   : ["//TODO",
                "return {defaultReturnType};"
               ],
    "footer" : "}}"
  },

  "main" : {
    "header" : "public static void main(String[] args) {{",
    "footer" : "}}",
    "parse_input" : [
      "String input = new Scanner(System.in).nextLine();",
      "JSONList argList = (JSONList) JSONParser.getObjectFromString(input);"
    ],

    "store_output" : "{returnType} output = {methodName}({actualsList});",
    "print_output" : "System.out.println(output);"
  },

  "args": {
    "float": "double {varName} = argList.getItem({argNum}).castToDouble();",
    "str" : "String {varName} = argList.getItem({argNum}).getData();",
    "int" : "int {varName} = argList.getItem({argNum}).castToInt();",
    "list_int" : [
                  "JSONList {varName}List = (JSONList) argList.getItem({argNum});",
                  "int[] {varName} = new int[{varName}List.getEntryCount()];",
                  "for (int {varName}index = 0; {varName}index < {varName}.length; {varName}index++)",
                  "{varName}[{varName}index] = {varName}List.getItem({varName}index).castToInt();"
                 ],
    "list_list_char" : 
      [
        "JSONList {varName}List = (JSONList) argList.getItem({argNum});",
        "JSONList {varName}ListInner = (JSONList) {varName}List.getItem(0);",
        "char[][] {varName} = new char[{varName}List.getEntryCount()][{varName}ListInner.getEntryCount()];",
        "for (int {varName}OuterIndex = 0; {varName}OuterIndex < {varName}List.getEntryCount(); {varName}OuterIndex++) {{",
        "JSONList innerList = (JSONList) {varName}List.getItem({varName}OuterIndex);",
        "for (int {varName}InnerIndex = 0; {varName}InnerIndex < innerList.getEntryCount(); {varName}InnerIndex++)",
        "{varName}[{varName}OuterIndex][{varName}InnerIndex] = innerList.getItem({varName}InnerIndex).castToChar();",
        "}}"
      ]
  },

  "default_values": {
    "int": "0",
    "float": "0.0",
    "str": "\"\"",
    "list_int": "new int[] {}}"
  },

  "types" : {
    "float": "double",
    "str" : "String",
    "int" : "int",
    "list_int" : "int[]",
    "list_list_char" : "char[][]"
  }

}
